#wrap
	height 640px
	width 1080px
	margin 0 auto
.content
	.content-main
		height 450px
		margin-top 20px
		.content-detail-wrap
			height 450px
			width 1080px
			display none


			.content-xinwendongtai-left,.content-xinwendongtai-right,.content-chanpinzixun-left,.content-chanpinzixun-right
				height 450px
				width 540px
				float left
		.actived
			display block
	.content-nav
		height 50px
		width 1080px
		.content-nav-wrap
			height 50px
			width 1080px
			.content-nav-item
				height 50px
				width 540px
				float left
				font-size 20px
				line-height 50px
				color #EEEEEE
				cursor pointer
				background-color: #034F2B
			.clicked
				background-color: #898000
.content-xinwendongtai-right
	.content-detail-list
		height 386px
		padding-left 50px
		li
			height 30px
			line-height 30px
			text-align left
			font-weight bold
			font-size 14px
			list-style-type square
			.list-item-wrap
				color #123
				.item-right
					float right
.content-chanpinzixun-left
	.content-detail-list
		height 386px
		padding-right 50px
		li
			height 30px
			line-height 30px
			text-align left
			font-weight bold
			font-size 14px
			list-style-type square
			.list-item-wrap
				color #123
				.item-right
					float right
#xw_pagination
	margin-left 50px
	border-top 1px dashed #111
#cp_pagination
	margin-right 50px
	border-top 1px dashed #111
.pagination
	.current
		color orange
	a, .prev, .next
		color #333