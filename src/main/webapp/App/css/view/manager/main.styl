header, footer {
  display none!important
}
.note-codable {
  display none
}
.header {
  height 50px
  width 1000px
  margin 0 auto
  text-align center

}
.content{
  width 1000px
  height 600px
  margin 0 auto
}
.nav{
  float left
  height 100%
  width 100px
}
.nav li {
  height 50px
  width 100px
  margin 10px
  background-color: #ff9667
  cursor pointer
}
.nav .now {
  height 50px
  width 100px
  background-color: #ff3e41
}
.edition-wrap {
  width 900px
  height 100%
  padding 50px
  float left
}
.note-editable {
  border solid
}
.news-wrap
  position absolute
  top 0
  left 0
  height 500px
  width 500px
  margin 0 auto
.news-main
  position fixed
  z-index 9999
  left 20%
  top 30px
  width 800px
  background #fff
  max-height 650px
  overflow-y scroll
  display block
.news-wrap-mask
  position fixed
  width 100%
  height 100%
  z-index 9998
  left 0
  right 0
  top 0
  bottom 0
  opacity 0.3
  background #000
.close-btn
  position fixed
  z-index 9999
  left 20%
  top 0
  height 30px
  width 30px
  color #fff
  background rgb(139,126,1)
  font-size 20px
  cursor pointer
.edition-step
  width 140px
  height 20px
  text-align left
.edition-head-input
  width 300px
  height 20
  display block
.edition-submit
  height 20px
  width 100px
  margin 10px
  float right
  background-color: #ff730f
  cursor pointer
.edition-preview
  height 20px
  width 100px
  margin 10px
  float right
  background-color: #ff730f
  cursor pointer

