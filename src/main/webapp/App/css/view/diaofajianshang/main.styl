.navbox
  position fixed
  z-index 10
#wrap
	height 100%
	min-width 1080px
	margin 0 auto
	padding-top 140px
.content-img
	width 100%
	position relative
	/*height 600px*/
	.content-main-nav
		width 180px
		height 600px
		position fixed
		top 0
		left 0
		float left
		.content-main-nav-mask-item
			width 180px
			float left
			color #eee
			font-size 16px
			height 50px
			cursor pointer
		.content-main-nav-wrap
			.content-main-nav-item
				width 180px
				color #eee
				font-size 16px
				line-height 50px
				cursor pointer
.content-main
	.peidaiyanshi-main
		height 600px
		width 900px
		margin 0 auto
		background-color #fff
	.peidaiyanshi-introduction
		float left
		height 600px
		width 450px
		padding 50px
		img
			height 200px
			width 340px
		.content-main-head
			height 45px
			border-bottom 1px solid #ddd
			.content-head-title
				font-size 28px
				border-bottom 5px solid #898000
				width 120px
				float left
			.content-head-title-eng
				font-size 16px
				display block
				text-align left
				line-height 52px
				color #bbb

	.article-introduction img
		width 100%
.content-wrap
	position relative
