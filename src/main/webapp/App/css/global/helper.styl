/* 字体属性 [定义规则，小写f加属性名称] */
.fB  
	font-weight: bold 
.fI
	font-style: italic 
.fx
	text-decoration: underline 
.fyh
	font-family: "微软雅黑" 
.fht
	font-family: "黑体" 
/* 字体大小 */
.f12px  
	font-size: 12px 
.f14px
	font-size: 14px 
.f16px
	font-size: 16px 
.hide
	display: none 
.bgPictureShow
	position: absolute
	height: 800px
	min-width 1200px
	left 0
	top 0
	z-index -1
	.picUl
		img
			height: 800px
			width 100%
.imgPos
	position fixed
	left 98%
	top 150px
	z-index 2
	li
		display: block
		cursor: pointer
#wrap
	height 660px
	width 1080px
	margin 0 auto
.navbox
	margin 0 auto
	font-size: 18px
	line-height: 36px
	color: #d8943a
	min-width: 1080px
	background-color: #006039

.navig
	background-color: #006039
	height: 100%
	float left
	cursor pointer
	color: #D8943A
	position relative
