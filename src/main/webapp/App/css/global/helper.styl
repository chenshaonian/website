
html 
	margin: 0
	padding: 0

body 
	text-align: center
	font-family: '微软雅黑'
	margin: 0
	padding: 0
	background: #fff
	font-size: 12px
	color: #404040
	overflow-y: scroll


div, form, img, ul, ol, li, dl, dt, dd, p 
	margin: 0
	padding: 0
	border: 0


body, div, h1, h2, h3, h4, h5, h6, p, ul, li, ol, dl, dt, dd, span, b, i, em, input, form, img, a, font 
	margin: 0
	padding: 0

table, td, tr, th 
	font-size: 12px

input, select 
	vertical-align: middle

.cl 
	clear: both

ul, li 
	list-style-type: none

textarea 
	font-size: 1em
	line-height: 18px
	overflow: hidden
	overflow-y: auto

/* 链接颜色 */
a:link 
	color: #2584B0
	text-decoration: none


a:visited 
	color: #2584B0
	text-decoration: none

a:hover 
	color: #2584B0
	text-decoration: underline

a:active 
	color: #2584B0

.fl_ft 
	float: left

.fl_rg 
	float: right

.clearfix:after 
	content: "."
	display: block
	height: 0
	clear: both
	visibility: hidden

.clearfix 
	display: inline-block


/* 颜色属性 [定义规则，小写c加颜色名称] */
.cGray, a.cGray:link, a.cGray:visited 
	color: #343434
	font-size: 14px


.cWhite, a.cWhite:link, a.cWhite:visited 
	color: #ffffff


a.cwhite:hover 
	color: #ffffff
